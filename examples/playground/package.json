{
  "name": "playground",
  "version": "1.0.0",
  "description": "Vue数学公式编辑器演示项目",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:gh-pages": "cross-env VITE_BASE_URL=/vue-mathjax-beautiful/ npm run build",
    "preview": "vite preview",
    "deploy": "npm run build:gh-pages && gh-pages -d dist"
  },
  "dependencies": {
    "lucide-vue-next": "^0.522.0",
    "vue": "^3.3.11",
    "vue-i18n": "^11.1.9",
    "vue-mathjax-beautiful": "workspace:*",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.32",
    "sass-embedded": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}